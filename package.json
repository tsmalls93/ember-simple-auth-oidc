{
  "name": "ember-simple-auth-oidc",
  "version": "1.1.0",
  "description": "A Ember Simple Auth addon which implements the OpenID Connect Authorization Code Flow.",
  "keywords": [
    "ember-addon"
  ],
  "license": "LGPL-3.0-or-later",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/adfinis-sygroup/ember-simple-auth-oidc",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.13.0",
    "ember-cli-htmlbars": "^4.2.2",
    "ember-get-config": "^0.2.4",
    "ember-simple-auth": "^2.1.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.2.0",
    "@adfinis-sygroup/semantic-release-config": "2.1.4",
    "@ember/jquery": "1.1.0",
    "@ember/optional-features": "1.3.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "~3.15.1",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-mirage": "1.1.6",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "2.0.2",
    "ember-cli-uglify": "3.0.0",
    "ember-data": "3.17.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.1",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.6.0",
    "ember-resolver": "7.0.0",
    "ember-source": "3.16.3",
    "ember-source-channel-url": "2.0.1",
    "ember-try": "1.4.0",
    "eslint": "6.8.0",
    "eslint-plugin-ember": "7.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "loader.js": "4.7.0",
    "prettier": "1.19.1",
    "qunit-dom": "1.0.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
